<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.test.banner"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        >
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.BannerActivity">
        </activity>
        <activity android:name=".activity.MultiBannerActivity">
        </activity>
        <activity android:name=".activity.LocationActivity">
        </activity>
        <activity android:name=".activity.TranslateActivity"
            android:theme="@style/AppTheme.Translate"
            >
        </activity>
        <activity android:name=".activity.TestActivity"/>
    </application>

</manifest>